msgstr "Arddull Statws Modd Mewnbwn GTK"

#: schemas/org.cafe.interface.gschema.xml:91
msgid "Name of the GTK+ input method Status Style used by ctk+."
msgstr "Enw'r arddull statws modd mewnbwn GTK+ a ddefnyddir gan ctk+."

#: schemas/org.cafe.interface.gschema.xml:95
msgid "GTK IM Module"
msgstr "Modiwl IM GTK"

#: schemas/org.cafe.interface.gschema.xml:96
msgid "Name of the input method module used by GTK+."
msgstr "Enw'r modiwl modd mewnbwn a ddefnyddir gan GTK+."

#: schemas/org.cafe.interface.gschema.xml:100
msgid "Use GTK3 header bar"
msgstr ""

